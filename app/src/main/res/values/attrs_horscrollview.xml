<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorScrollSelectedView">
        <!--可见数目-->
        <attr name="nums_in_horizon" format="integer"/>
        <!--选中状态-->
        <attr name="selected_text_size" format="float"/>
        <attr name="selected_text_color" format="color|reference"/>
        <!--默认状态-->
        <attr name="normal_text_size" format="float"/>
        <attr name="normal_text_color" format="color|reference"/>
    </declare-styleable>
</resources>